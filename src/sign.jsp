<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<!DOCTYPE html>
<html testmanifest="/app.appcache">
  <head>
    <meta charset="utf-8" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, height=device-height" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/static/v3/h5/login_mobile.css?v=2" />

    <script src="/static/v3/h5/compressed.js"></script>
    <script src="/static/v3/h5/m.js?v=1e"></script>
    <script src="/config.js"></script>
    <title>APP登录</title>
  </head>
  <body>
    <div class="login_logo"><img src="data:image/png;base64," alt="" width="242" height="57"/></div>
    <h1 class='mobile_login'>欢迎登陆头彩娱乐</h1>

    <div id="loginform" v-on:keydown.13="fire">
      <a href="/activity/0" class="hrefActivity" id="hrefActivity"></a>
      <ul v-show="!loginType">
        <li><em class="iconic">&#xe600;</em><input type="text" v-on:keyup="check" v-model="name" id="login-name" placeholder="用户名"></li>
        <li><em class="iconic">&#xe641;</em><input type="password" v-on:keyup="checkpwd" v-model="pass" id="login-pass" placeholder="密码"></li>
        <li class="captchali" v-show="show"><em class="iconic">&#xe603;</em>
    <input style='margin-left:42px' type="text" v-on:keyup="checkcode" v-model="captcha" id="captcha-input" placeholder="验证码">
    <img v-bind:src="captchasrc" alt="" id="captcha-img" width="" height="" /></li>{{login_pass_error}}
        <li class="nobottom remember remembered" id="rememberme" @click="switchreme"><span class="circle"></span><em class="m_ico" id="rememberme_ico">✔</em><label>记住密码</label></li>
        <li class="nobottom"><a class="btn hand" id="start_login" v-on:click="login">登 录</a></li>
      </ul>
      <ul class="mobile_login_mobile" v-show="loginType">
        <li><em class="icon">&#xe600;</em><input type="text" v-model="mobile" placeholder="手机号"></li>
        <li class="captchali verifyImg">
          <input type="text" v-model="verifyImg" placeholder="图片验证码">
          <img @click="RefreshSafety" v-bind:src="safetySrc" alt=""  width="" height="" />
        </li>
        <li class="captchali verifyCode">
          <input type="text" v-model="verifyCode" placeholder="手机验证码">
          <span @click="get_verify_code()">
            <i v-show="!isCountFinish">发送验证码</i>
            <i v-show="isCountFinish">{{countTime}}</i>
          </span>
        </li>
        <li class="error colorA ft12 main_error" v-cloak>{{login_pass_error}}</li>
        <li class="nobottom"><a class="btn hand" v-on:click="login">登 录</a></li>
      </ul>
      <!--<div class="switch_cont">
        <label class="lf" v-show="!loginType" @click="switchLogin(1)">切换为手机号登录</label>
        <label class="lf" v-show="loginType" @click="switchLogin(0)">切换为账号密码登录</label>
      </div>-->
    </div>
    <div id="foot_link">
      <div class="content">
        <a href="http://toucaiyl.com/register" id="registerlink" class="lf">注册会员</a>
        <a v-bind:href="mlink" class="rf" target="_blank">咨询客服</a>
      </div>
    </div>
    <script type="text/javascript">
      (function (){

        var registerlink = document.getElementById("registerlink");
        var loginform = document.getElementById("loginform");
        var hrefActivity = document.getElementById("hrefActivity");
        var urls = location.href;
        let url = urls.split('referrer=')[1];
        let referrer = JSON.parse(localStorage.getItem('_referrer'));
        let delReferrer = appConfig().referrerMap;
        // || (referrer && referrer !== 'false'
        if(url){
            registerlink.href = '/register?referrer=' + url + '&smobile=web';
            for(let i = 0, l = delReferrer.length; i < l; i++) {
              if(url === delReferrer[i]) {
                return false;
              }
            }
            loginform.className="referrerWarp";
            hrefActivity.style.display="block";
        }else{
            registerlink.href = '/register' + '?smobile=web';
        }
      })();
    </script>
  </body>
</html>
